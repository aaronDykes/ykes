
class String {}

var str = String();

class Fizzbang
{

    res(n)
    {
        for (var i = 1; i < n; i++)
            if ((i % 15) == 0)
            {
                str.fb = "Fizzbang: " + num -> string i;
                pout(str.fb);
            }
            elif ((i % 5) == 0)
            {
                str.bang = "Bang: " + num -> string i;
                pout(str.bang);
            }
            elif ((i % 3) == 0)
            {
                str.fizz = "Fizz: " + num -> string i;
                pout(str.fizz);
            }
            else
                pout(i);

    }
}



var fizz = Fizzbang();
var time = clock();
fizz.res(100);
pout(clock() - time);

pout(
    `fizzbang to_string:
    ${str.fb},
    ${str.bang},
    ${str.fizz}`
);

rm(fizz);
rm(str);



