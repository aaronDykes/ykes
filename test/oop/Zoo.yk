class Zoo
{
    init() 
    {
        this.aardvark = 1;
        this.baboon   = 1;
        this.cat      = 1;
        this.donkey   = 1;
        this.elephant = 1;
        this.fox      = 1;
    }
    ant()    { return this.aardvark; }
    banana() { return this.baboon; }
    tuna()   { return this.cat; }
    hay()    { return this.donkey; }
    grass()  { return this.elephant; }
    mouse()  { return this.fox; }

    res()
    {
        this.sum = 0;

        for (; this.sum < 10000000000; ) {
            this.sum += this.aardvark
                + this.elephant
                + this.baboon
                + this.cat
                + this.donkey
                + this.elephant;
        }
        pout(
            this.sum
        );
    }

}



