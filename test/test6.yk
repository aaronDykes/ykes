
sr ternary()
{

    var hello = (420 == 69) ? "nope" : "that works";

    pout(
        hello,
        (69 == 69) ? "hello world" : "okay then"
    );

    return hello;

}

sr null_coalescing()
{
    var ab  = ternary;
    var b = ternary();
    var a = String("Hello world!");

    var hello = b ?? a;

    pout((hello == "that works") ?  ab() : "another string");
}

