sr sum(num1, num2)
{
    return num1 + num2;
}

sr arrays()
{
    var ar = ["Hello", "World", "Vector", "of", "Vectors"];

    for (var i = 0; i < ar.len; i++)
        pout(ar[i]);


    rm(ar); 
}

sr string_access()
{
    
    var ar = ["Hello", "World", "Vector", "of", "Vectors"];
    
    ar[0] = "Aaron";
    ar[1] = "Dykes"; 

    ar.push("Another vector!!");
    ar.push("VECTORS!!");



    var i = ar.pop();
 
    pout(
        ar,
        i,
        ar.pop(),
        ar,
        ar.len
    );
 

    rm(ar);
}


arrays();
string_access();

 